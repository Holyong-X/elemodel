<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.holyong.elemodel.mapper.RiverMapper">



    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.holyong.elemodel.entity.River">
        <id column="river_id" property="riverId" />
        <result column="SMID" property="smid" />
        <result column="river_name" property="riverName" />
        <result column="river_length" property="riverLength" />
        <result column="water_lever" property="waterLever" />
        <result column="flow" property="flow" />
        <result column="direction" property="direction" />
        <result column="respons" property="respons" />
        <result column="phone" property="phone" />
        <result column="post" property="post" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        river_id, SMID, river_name, river_length, water_lever, flow, direction, respons, phone, post
    </sql>

    <!--自定义关联查询-->
    <!-- 表名 -->
    <sql id="t_name">river</sql>
    <!-- 别名 -->
    <sql id="t_alias">river as river</sql>
    <!--外键表关联定义 -->
    <sql id="join"></sql>
</mapper>
