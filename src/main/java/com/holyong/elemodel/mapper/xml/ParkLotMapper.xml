<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.holyong.elemodel.mapper.ParkLotMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.holyong.elemodel.entity.ParkLot">
        <id column="id" property="id" />
        <result column="smid" property="smid" />
        <result column="parkNumber" property="parkNumber" />
        <result column="parkName" property="parkName" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, smid, parkNumber, parkName
    </sql>

    <!--自定义关联查询-->
    <!-- 表名 -->
    <sql id="t_name">park_lot</sql>
    <!-- 别名 -->
    <sql id="t_alias">park_lot as lot</sql>
    <!--外键表关联定义 -->
    <sql id="join"></sql>
</mapper>
