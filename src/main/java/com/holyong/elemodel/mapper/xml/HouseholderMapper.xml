<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.holyong.elemodel.mapper.HouseholderMapper">



    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.holyong.elemodel.entity.Householder">
        <id column="householder_id" property="householderId" />
        <result column="room_id" property="roomId" />
        <result column="name" property="name" />
        <result column="sex" property="sex" />
        <result column="age" property="age" />
        <result column="certificate_number" property="certificateNumber" />
        <result column="relation" property="relation" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        householder_id, room_id, name, sex, age, certificate_number, relation
    </sql>

    <!--自定义关联查询-->
    <!-- 表名 -->
    <sql id="t_name">householder</sql>
    <!-- 别名 -->
    <sql id="t_alias">householder as householder</sql>
    <!--外键表关联定义 -->
    <sql id="join"></sql>
</mapper>
